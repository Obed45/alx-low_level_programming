import ctypes

# Load the dynamic library
lib = ctypes.CDLL('./liboperations.so')

# Call the C functions and print the results
a = 5
b = 3
print(f"{a} + {b} = {lib.add(a, b)}")
print(f"{a} - {b} = {lib.sub(a, b)}")
print(f"{a} x {b} = {lib.mul(a, b)}")
print(f"{a} / {b} = {lib.div(a, b)}")
print(f"{a} % {b} = {lib.mod(a, b)}")
