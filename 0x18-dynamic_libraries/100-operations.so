import ctypes

my_library = ctypes.CDLL('./my_library.so')

a = 10
b = 5

print("Addition: ", my_library.add(a, b))
print("Subtraction: ", my_library.subtract(a, b))
print("Multiplication: ", my_library.multiply(a, b))
print("Division: ", my_library.divide(a, b))
print("Modulus: ", my_library.modulus(a, b))
